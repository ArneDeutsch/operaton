<?xml version="1.0" encoding="UTF-8"?>
<definitions 
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:operaton="http://operaton.org/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="exclusive" isExecutable="true">
  
    <startEvent id="theStart" />
    
    <sequenceFlow sourceRef="theStart" targetRef="service" />    
    
    <serviceTask id="service" operaton:expression="${true}" operaton:async="true" operaton:exclusive="false" />
    
    <sequenceFlow sourceRef="service" targetRef="theEnd" />
        
    <endEvent id="theEnd" />
    
  </process>

</definitions>

<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:operaton="http://operaton.org/schema/1.0/bpmn"
             targetNamespace="Examples">

  <process id="process" isExecutable="true">

    <startEvent id="theStart">
      <extensionElements>
        <operaton:executionListener event="start" class="org.operaton.bpm.engine.test.standalone.entity.ExecutionOrderListener" />
      </extensionElements>
    </startEvent>

    <sequenceFlow sourceRef="theStart" targetRef="theService" />

    <serviceTask id="theService" operaton:expression="${true}">
      <extensionElements>
        <operaton:executionListener event="start" class="org.operaton.bpm.engine.test.standalone.entity.ExecutionOrderListener" />
      </extensionElements>
    </serviceTask>

    <sequenceFlow sourceRef="theService" targetRef="fork" />

    <parallelGateway id="fork">
      <extensionElements>
        <operaton:executionListener event="start" class="org.operaton.bpm.engine.test.standalone.entity.ExecutionOrderListener" />
      </extensionElements>
    </parallelGateway>

    <sequenceFlow sourceRef="fork" targetRef="theService1" />
    <serviceTask id="theService1" operaton:expression="${true}">
      <extensionElements>
        <operaton:executionListener event="start" class="org.operaton.bpm.engine.test.standalone.entity.ExecutionOrderListener" />
      </extensionElements>
    </serviceTask>
    <sequenceFlow sourceRef="theService1" targetRef="theEnd1" />
    <endEvent id="theEnd1" operaton:asyncBefore="true">
      <extensionElements>
        <operaton:executionListener event="start" class="org.operaton.bpm.engine.test.standalone.entity.ExecutionOrderListener" />
      </extensionElements>
    </endEvent>

    <sequenceFlow sourceRef="fork" targetRef="theService2" />
    <serviceTask id="theService2" operaton:expression="${true}">
      <extensionElements>
        <operaton:executionListener event="start" class="org.operaton.bpm.engine.test.standalone.entity.ExecutionOrderListener" />
      </extensionElements>
    </serviceTask>
    <sequenceFlow sourceRef="theService2" targetRef="theService3" />
    <serviceTask id="theService3" operaton:expression="${true}">
      <extensionElements>
        <operaton:executionListener event="start" class="org.operaton.bpm.engine.test.standalone.entity.ExecutionOrderListener" />
      </extensionElements>
    </serviceTask>
    <sequenceFlow sourceRef="theService3" targetRef="theEnd2" />
    <endEvent id="theEnd2" operaton:asyncBefore="true">
      <extensionElements>
        <operaton:executionListener event="start" class="org.operaton.bpm.engine.test.standalone.entity.ExecutionOrderListener" />
      </extensionElements>
    </endEvent>

  </process>

</definitions>
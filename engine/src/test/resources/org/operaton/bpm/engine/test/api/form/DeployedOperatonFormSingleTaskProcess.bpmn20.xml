<?xml version="1.0" encoding="UTF-8" ?>

<definitions id="definitions"
             targetNamespace="http://operaton.org/schema/1.0/bpmn20" 
             xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:operaton="http://operaton.org/schema/1.0/bpmn">
  
  <process id="FormsProcess" isExecutable="true">
  
    <startEvent id="start"/>

    <sequenceFlow id="flow1" sourceRef="start" targetRef="task" />
    
    <userTask id="task"
              operaton:formKey="operaton-forms:deployment:org/operaton/bpm/engine/test/api/form/task.html"
              operaton:assignee="kermit" />

    <sequenceFlow id="flow2" sourceRef="task" targetRef="wait" />
    
    <receiveTask id="wait" />

  </process>
  
</definitions>
